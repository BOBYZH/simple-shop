{
  "name": "simple-shop",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "npx semistandard --fix || npx nodemon ./bin/www",
    "seed": "node seeders/_DB && node seeders/_tables && node seeders/members && node seeders/products",
    "test": "npx semistandard --fix || node seeders/_DB && node seeders/_tables && npx mocha tests --exit --timeout 5000",
    "cover": "npx nyc npm run test"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "bcryptjs": "^2.4.3",
    "connect-flash": "^0.1.1",
    "cookie-parser": "~1.4.4",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.11.2",
    "debug": "~2.6.9",
    "dotenv": "^16.0.0",
    "ejs": "~2.6.1",
    "ejs-locals": "^1.0.2",
    "express": "~4.16.1",
    "express-session": "^1.17.2",
    "http-errors": "~1.6.3",
    "mariadb": "^3.0.0",
    "method-override": "^3.0.0",
    "morgan": "~1.9.1",
    "sql-template-strings": "^2.2.2",
    "uuid4": "^2.0.2"
  },
  "devDependencies": {
    "chai": "^4.3.6",
    "mocha": "^10.0.0",
    "nodemon": "^2.0.16",
    "nyc": "^15.1.0",
    "semistandard": "^16.0.1",
    "sinon": "^14.0.0",
    "sinon-chai": "^3.7.0",
    "supertest": "^6.2.3"
  }
}
